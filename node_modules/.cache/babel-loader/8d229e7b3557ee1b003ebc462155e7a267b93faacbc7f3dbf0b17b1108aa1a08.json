   );\n            this.upcomingMovies = response.data.results.slice(1, 6);\n            console.log(this.upcomingMovies);\n        }\n    }\n}\n</script>\n\n<style>\n.carousel-3d-container figure {\n    margin: 0;\n}\n.carousel-3d-container figcaption {\n    position: absolute;\n    background-color: rgba(0, 0, 0, 0,5);\n    color: white;\n    bottom: 0;\n    padding: 15px;\n    font-size: 12px;\n    min-width: 100%;\n    box-sizing: border-box;\n}\n\n.next span,\n.prev span {\n    color: red;\n}\n</style>"],"mappings":"AAuBA,SAAAA,UAAA,EAAAC,KAAA;AACA;EACAC,KAAA;IACA;MACAC,cAAA;IACA;EACA;EACAC,UAAA;IACAJ,UAAA;IACAC;EACA;EACAI,QAAA;IACA,KAAAC,mBAAA;EACA;EACAC,OAAA;IACA,MAAAD,oBAAA;MACA,MAAAE,QAAA,cAAAC,KAAA,CAAAC,GAAA,CACA,6CACA;MACA,KAAAP,cAAA,GAAAK,QAAA,CAAAN,IAAA,CAAAS,OAAA,CAAAC,KAAA;MACAC,OAAA,CAAAC,GAAA,MAAAX,cAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}